{
  "name": "vue-ui-kit-helper",
  "displayName": "vue-ui-kit-helper",
  "description": "结合 vue-snippet-gen，指定组件库可自动启用自动补全能力",
  "version": "0.0.4",
  "publisher": "engvuchen",
  "icon": "helper.png",
  "engines": {
    "vscode": "^1.59.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/engvuchen/vue-ui-kit-helper"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vue Ui Kit Helper Configuration",
      "properties": {
        "vue-ui-kit-helper.sources": {
          "type": "array",
          "default": [
            "element-test-demo"
          ],
          "description": "Data Sources in .vscode"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {}
}
